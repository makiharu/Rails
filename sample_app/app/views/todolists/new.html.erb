<h1>新規登録</h1>

<%= form_for(@list, url:todolists_path) do |f| %>
  <h3>タイトル</h3>
  <%= f.text_field :title %>
  <h3>本文</h3>
  <%= f.text_area :body %>
  <h3>画像</h3>
  <%= f.attachment_field :image %>

  <%= f.submit "投稿" %>

<% end %>


<%= link_to "topページ", root_path %>
<%= link_to "投稿一覧", todolists_path %>